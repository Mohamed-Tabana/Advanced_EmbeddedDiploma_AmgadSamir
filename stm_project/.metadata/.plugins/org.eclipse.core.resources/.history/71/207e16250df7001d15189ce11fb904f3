#include <stdint.h>



#include <stdio.h>
#include<stdlib.h>
#include<string.h>

void SVC_Handler()
{
	printf("DDDDD");
}
void GenerateInterrupt()
{
	__asm volatile ( "SVC #0x02" );
}
void ChangeIntoPSP()
{
	__asm volatile ( ".equ SRAM_END, (0x20000000U + (128*1024))" );
	__asm volatile ( ".equ PSP_END ,  SRAM_END - 512 " );
	//
	//	/* initialize PSP*/
	__asm volatile ( "LDR R0, =PSP_END" );
	__asm volatile ( "MSR PSP, R0" );

	/* make SP = PSP */
	__asm volatile ("LDR R0, =#0x02");
	__asm volatile ("MSR CONTROL, R0");

}

void add(int x,int y,int z,int w)
{
	return x+y+z+w;
}

int main()
{
	ChangeIntoPSP();
	add(1,2,3,4);

	GenerateInterrupt();



	return 0;
}

