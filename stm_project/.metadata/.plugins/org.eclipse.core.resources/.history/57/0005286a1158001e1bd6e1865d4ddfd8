
#include "STM32F446xx.h"
#include "I2C_int.h"
#include "Utils.h"
#include "ErrType.h"
#include "RTC.h"
#define RTC_ADDRESS (uint8_t)0b01101000
uint8_t RTC_prg_Array[]=
{
	00,			//RTC_SECONDS;
	00,         //RTC_MINUTES;
	12,         //RTC_HOURS;
	THURSDAY,   //RTC_DAY;
	21,         //RTC_DATE;
	09,         //RTC_MONTH;
	23          //RTC_YEAR;
};

void RTC_Programming(I2C_HandleTypeDef_t* hi2c,DMA_HandleTypeDef_t* copy_eDMAConfig)
{
	I2C_SendDataToSlave_DMA(hi2c, copy_eDMAConfig,uint8_t copy_u8SlaveAdd,uint32_t copy_u32DataLength,uint8_t* copy_pu8Data);

}
