#include <stdint.h>

#include "GPIO_Interface.h"
#include "RCC_Interface.h"
#include "SYSTICK_Interface.h"
PinConfig_t myPin =
{
	.Mode=OUTPUT_MSPEED,
	.Output_Type=GP_PUSH_PULL,
	.Pin_Num=PIN13,
	.Port_Num=PORTA,

}
int main(void)
{
